
.topfem {
	width: 350px;
	margin: auto; 
	background: #ededed;
	overflow: auto;
	margin-top: 10px;
	margin-bottom: 10px;
	border: 1px solid #999;
}



#tags_field{
	display: block;
}


.topfem ul{
	list-style-type: none;
	padding-left: 0px;
}

.topfem form{
	padding-left: 35px;
}

.topfem label{
	font-family: "Arial Black", sans-serif;
	font-size: 25px;
	color: #0087bf;
	text-shadow: 1px 1px 0px black;
	margin-right: 10px;
}

.topfem h1{
	font-size: 25px;
	color: #626262;
	font-family: "Arial Black", sans-serif;
}

.create_list {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 11px;
	height: 30px;
	width: 240px;
	margin-bottom: 10px;
	color: #626262;
	padding-left: 7px;
}

#create_rubrik{
	height: 40px;
	width: 276px;
	font-size: 20px;
	font-family: sans-serif;
	color: #808080;
	padding-left: 7px;
}

#subbut{
	height: 40px;
	width: 100px;
	float: right;
	margin-bottom: 10px;
	margin-right: 10px;
	font-size: 15px;
	color: #505050;
	text-shadow: 1px 1px 0px white;
}
